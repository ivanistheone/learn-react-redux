{
  "name": "learn-react-redux",
  "version": "0.1.0",
  "description": "Step-by-step buildup of a react/redux project",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "NODE_ENV='production' node_modules/.bin/webpack --config webpack.config.js --bail -p",
    "dev": "NODE_ENV='development' node_modules/.bin/webpack-dev-server --content-base frontend/build/ --inline --hot --progress",
    "lint": "node_modules/.bin/eslint --ignore-pattern **/bundle*.js --ext .js --ext .jsx frontend/",
    "open": "open http://localhost:8080/frontend/build/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivanistheone/learn-react-redux.git"
  },
  "keywords": [
    "es2015",
    "react",
    "redux"
  ],
  "author": "Ivan Savov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ivanistheone/learn-react-redux/issues"
  },
  "homepage": "https://github.com/ivanistheone/learn-react-redux#readme",
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "file-loader": "^0.8.5",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1",
    "eslint": "^1.10.3",
    "babel-eslint": "^5.0.0-beta6",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-react": "^3.16.1"
  }
}
